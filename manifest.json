{
  "manifest_version": 2,
  "name": "Tumblr Likes - Missing Button",
  "version": "2.1.2",
  "description": "A simple extension that adds a Tumblr Likes button that goes to a user's 'Liked By' page.",
  "permissions": [
    "http://*.tumblr.com/", "http://*.tumblr.com/*"
  ],
  "icons": {
                  "16": "icon16.png",
                  "48": "icon48.png",
                  "128": "icon128.png" 
           },
  "content_scripts": [
    {
      "matches": ["http://*.tumblr.com/*"],
      "js": ["jquery.min.js", "background.js"],
      "css": ["css.css"],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://assets.tumblr.com/assets/html/iframe/o.html*", "http://www.tumblr.com/dashboard/iframe*"],
      "js": ["jquery.min.js", "test.js"],
      "css": ["css.css"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "images/*"
  ]
}

